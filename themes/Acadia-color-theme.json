	"tokenColors": [{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#ABC6E6",
				"fontStyle": "italic"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#B2BA3B"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation.definition",
			"settings": {
				"foreground": "#40B59B"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#EE9B02"
			}
		},
		{
			"name": "Built-in constant",
